<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>WinUIEssential.UWP</id>
        <version>1.1.0</version>
        <title>WinUIEssential(UWP)</title>
        <authors>Peter</authors>
        <owners></owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Essential helpers for developing WinUI(UWP)</description>
        <dependencies>
            <dependency id="Microsoft.UI.Xaml" version="2.7.0"/>
        </dependencies>
    </metadata>
    <files>
        <!--build/native-->
        <file src="include/Glyphs.hpp" target="build/native/include"/>
        <file src="include/ToastBuilder.hpp" target="build/native/include"/>
        <file src="include/ToastTemplates.hpp" target="build/native/include"/>
        <file src="include/PropertyChangeHelper.hpp" target="build/native/include"/>
        <file src="include/BadgeGlyphs.hpp" target="build/native/include"/>
        <file src="include/WinUIIncludes.hpp" target="build/native/include"/>
        <file src="include/Taskbar.h" target="build/native/include"/>
        <file src="include/TemplateControlHelper.hpp" target="build/native/include"/>
        <file src="Export.h" target="build/native/include"/>
        <file src="include/WindowHelper.hpp" target="build/native/include"/>
        <file src="WinUIEssential.UWP.targets" target="build/native"/>
        
        <!--lib/native-->
        <file src="..\x64\Debug\UWPPackage\UWPPackage.winmd" target="lib/native"/>
        <file src="..\x64\Debug\UWPPackage\UWPPackage.dll" target="lib/native"/>
        <file src="..\x64\Debug\UWPPackage\UWPPackage.lib" target="lib/native/Debug/x64"/>
        
        <!--control resources-->
        <file src="..\x64\Debug\UWPPackage\UWPPackage\*.*" target="lib/native"/>
    </files>
</package>